#compdef pecos
local -a subcmds
subcmds=('cdr:search cdr history and change directory'\
         'commit-hash:search git log and copy commit-hash'\
         'docker-compose:search and download docker-compose.yml'\
         'history:search history'\
         'kill:select and kill process'\
         'rails:search Rails template and initialize Rails project'\
         'search:search find and directory in the current directory')
_describe 'command' subcmds
